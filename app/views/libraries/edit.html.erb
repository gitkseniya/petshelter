<form action="/libraries/<%= @library.id %>" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="hidden" name="_method" value="PATCH" />
  <p>Edit</p>
  <input type='text' name='library[name]' value="<%= @library.name %>"/><br/>
  <input type='number' name='library[district_num]' value="<%= @library.district_num %>"/><br/>
    <input type='text' name='library[public]' value="<%= @library.public %>"/><br/>
  <input type='submit' name='submit'/>
</form>
