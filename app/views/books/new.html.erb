<%= form_with url: "/libraries/#{@library.id}/books", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br>
  <%= form.label :author %>
  <%= form.text_field :author %><br>
  <%= form.label :rating %>
  <%= form.text_field :rating %><br>
  <%= form.label :available %>
  <%= form.text_field :available %><br>

  <%= form.submit 'Create Book' %>
<% end %>
